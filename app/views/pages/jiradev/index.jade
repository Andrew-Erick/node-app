script(src="//ajax.googleapis.com/ajax/libs/jquery/1.8.3/jquery.min.js")
script(src="//aui-cdn.atlassian.com/aui-adg/6.0.7/js/aui.min.js")
script(src="//aui-cdn.atlassian.com/aui-adg/6.0.7/js/aui-experimental.min.js")
